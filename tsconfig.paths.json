{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@constants": ["src/constants"],
      "@constants/*": ["src/constants/*"],
      "@layout": ["src/layout"],
      "@layout/*": ["src/layout/*"],
      "@typings": ["src/typings"],
      "@typings/*": ["src/typings/*"],
      "@redux": ["src/redux"],
      "@redux/*": ["src/redux/*"]
    }
  }
}
