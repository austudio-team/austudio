{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@constants": ["src/constants"],
      "@constants/*": ["src/constants/*"],
      "@layout": ["src/layout"],
      "@layout/*": ["src/layout/*"],
      "@typings": ["src/typings"],
      "@typings/*": ["src/typings/*"],
      "@redux": ["src/redux"],
      "@redux/*": ["src/redux/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@events": ["src/events"],
      "@events/*": ["src/events/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@audio": ["src/audio"],
      "@audio/*": ["src/audio/*"]
    }
  }
}
